<a href="contents/<%= row.storageId %>" class="storage-card">
    <%if(row.image) {%>
    <img src="<%=row.image%>" class="card-background" />
    <% }%>
    <div class="card-background-gradient"></div>
    <div class="card-content">
        <div class="card-content-left">
            <% if(isAuthenticated && typeof isFavourite != "undefined") { %>
            <div class="card-favourite <%if(isFavourite){%>active<%}%> material-icons" 
                data-id="<%= row.storageId %>">favorite</div>
            <% } %>
        </div>
        <div class="card-content-right">
            <div class="card-main">
                <div class="card-title"><%=row.title%></div>
                <% if(row.storageType == 1) {%>
                <div class="card-storage-type">community fridge</div>
                <% } else { %>
                <div class="card-storage-type">community pantry</div>
                <% } %>
                <span class="card-rating-container">
                    <span class="card-rating">4.5/5</span>
                    <span class="card-rating-icon material-icons">star</span>
                </span>
            </div>
            <span class="card-footer">
                <% if (row.restocked) { %>
                <div class="card-footer-restocked"><span>RESTOCKED</span></div>
                <% } %>
                <% if(typeof distance != "undefined") { %>
                <div class="card-distance"><%= distance %> km</div>
                <% } %>
            </span>
        </div>
    </div>
</a>